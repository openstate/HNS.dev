<form>
	<table>
		<tbody>
			<tr><th>##user.rights##:</th></tr>
			<e:field>
				<label>##user.rights.read##:</label>
				<content>
					<e:checkbox name="read" />
				</content>
			</e:field>

			<e:field>
				<label>##user.rights.write##:</label>
				<content>
					<e:checkbox name="write" />
				</content>
			</e:field>

			<e:field>
				<label />
				<content>
					<input type="submit" value="##user.save##" />
				</content>
			</e:field>
		</tbody>
	</table>
</form>